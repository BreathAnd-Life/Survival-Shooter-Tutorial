using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class CreateCube : MonoBehaviour {

	public Transform cubeArray; 

	void Start () {
		for (int i = 0; i < 20; i++) 

		{

			Instantiate(cubeArray, new Vector3(Random.Range(-10f,10f),1,Random.Range(-10f,10f)), Quaternion.identity);

		}

	}

}
